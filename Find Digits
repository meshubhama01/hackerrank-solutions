#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int t; 
    scanf("%d",&t);
    for(int a = 0; a < t; a++){
        int n,count=0,b; 
        scanf("%d",&n);
        
           int p=n;
        while(p > 0)
            {
            b=p%10;
          if(b!= 0)  
          {if(n%b==0)
                count=count+1;
          }
            p=p/10;
              
            
        }  
        
        printf("%d\n",count);
       
        
    }
    return 0;
}
